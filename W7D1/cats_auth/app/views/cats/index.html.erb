<h1>Cats upon cats!</h1>
<ul>
  <% @cats.each do |cat| %>
    <li><a href="<%= cat_url(cat) %>"><%= cat.name %></a></li>
  <% end %>
</ul>

<% if logged_in? %>
  <form action="<%= new_cat_rental_request_url %>" method="get">
      <input type="submit" value="Submit Rental Request">
  </form>
  <br>
  <a href="<%= new_cat_url %>">Create cat</a>
<% end %>

